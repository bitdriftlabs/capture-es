{
  "name": "@bitdrift/react-native",
  "version": "0.7.0",
  "description": "bitdrift integration for React Native",
  "main": "dist/commonjs/index",
  "module": "dist/module/index",
  "types": "dist/typescript/packages/react-native/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "src/NativeBdReactNative.ts",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitdriftlabs/capture-es.git"
  },
  "author": "bitdrift <info@bitdrift.io> (https://bitdrift.io)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/bitdriftlabs/capture-es/issues"
  },
  "homepage": "https://github.com/bitdriftlabs/capture-es#readme",
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@3.6.1",
  "codegenConfig": {
    "name": "RNBdReactNativeSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.bdreactnative"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.lib.json",
          "configFile": true
        }
      ]
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "^9.0.14",
    "fast-json-stringify": "^6.0.0"
  }
}
